<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/main_logo"
        android:label="뮤직메이트"
        android:roundIcon="@drawable/main_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.DasoniAPP"
        tools:targetApi="31">
        <activity
            android:name=".RhythmMenuActivity"
            android:exported="false"
            android:screenOrientation="landscape" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".LoginResultActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="false" />
        <activity
            android:name=".RhythmGameActivity"
            android:exported="false"
            android:screenOrientation="landscape" />
        <activity
            android:name=".PlayDrumActivity"
            android:exported="false"
            android:screenOrientation="landscape" />
        <activity
            android:name=".ScoreGameActivity"
            android:exported="false"
            android:screenOrientation="landscape" />
        <activity
            android:name=".PlayMusicActivity"
            android:exported="false"
            android:screenOrientation="landscape" />
        <activity
            android:name=".ReadNoteActivity"
            android:exported="false"
            android:label="@string/title_activity_read_note"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.DasoniAPP" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>