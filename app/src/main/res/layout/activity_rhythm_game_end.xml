<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#40000000">

    <ImageView
        android:id="@+id/imageView95"
        android:layout_width="390dp"
        android:layout_height="370dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="181dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="181dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rhythm_game_end_background" />

    <ImageView
        android:id="@+id/end_star"
        android:layout_width="129dp"
        android:layout_height="58dp"
        android:layout_marginTop="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView95"
        app:layout_constraintEnd_toEndOf="@+id/imageView95"
        app:layout_constraintStart_toStartOf="@+id/imageView95"
        app:layout_constraintTop_toTopOf="@+id/imageView95"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/rhythm_game_end_twostar" />

    <ImageView
        android:id="@+id/song_menu_button"
        android:layout_width="152dp"
        android:layout_height="64dp"
        android:layout_marginBottom="45dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView95"
        app:layout_constraintEnd_toEndOf="@+id/imageView95"
        app:layout_constraintStart_toStartOf="@+id/imageView95"
        app:layout_constraintTop_toTopOf="@+id/imageView95"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/rhythm_game_retry_button" />

    <TextView
        android:id="@+id/textView125"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="120dp"
        android:fontFamily="@font/im_hyemin_bold"
        android:text="Good Job!"
        android:textColor="#489BFF"
        android:textSize="33sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView95"
        app:layout_constraintEnd_toEndOf="@+id/imageView95"
        app:layout_constraintStart_toStartOf="@+id/imageView95"
        app:layout_constraintTop_toTopOf="@+id/imageView95"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView126"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/im_hyemin_regular"
        android:text="점수"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/textView127"
        app:layout_constraintEnd_toEndOf="@+id/imageView95"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView95"
        app:layout_constraintTop_toBottomOf="@+id/textView125"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView127"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginBottom="18dp"
        android:fontFamily="@font/im_hyemin_regular"
        android:text="최고 점수"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/song_menu_button"
        app:layout_constraintEnd_toEndOf="@+id/imageView95"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView95"
        app:layout_constraintTop_toBottomOf="@+id/textView125"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/curr_score_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/im_hyemin_bold"
        android:text="157"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/textView129"
        app:layout_constraintEnd_toEndOf="@+id/imageView95"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView126"
        app:layout_constraintTop_toBottomOf="@+id/textView125"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView129"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="100dp"
        android:fontFamily="@font/im_hyemin_bold"
        android:text="999"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/song_menu_button"
        app:layout_constraintEnd_toEndOf="@+id/imageView95"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView127"
        app:layout_constraintTop_toBottomOf="@+id/textView125"
        app:layout_constraintVertical_bias="0.753" />
</androidx.constraintlayout.widget.ConstraintLayout>