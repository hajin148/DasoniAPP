<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/et_phone"
        android:layout_width="298dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="15dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/pretendard_regular"
        android:hint="휴대폰 번호를 입력해주세요"
        android:textColor="#C4C4C4"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView23"
        app:layout_constraintStart_toEndOf="@+id/imageView41"
        app:layout_constraintTop_toTopOf="@+id/imageView41"
        app:layout_constraintVertical_bias="0.125" />

    <ImageView
        android:id="@+id/imageView33"
        android:layout_width="330dp"
        android:layout_height="2dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView24"
        app:srcCompat="@drawable/login_line" />

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="330dp"
        android:layout_height="2dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView35"
        app:srcCompat="@drawable/login_line" />

    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="330dp"
        android:layout_height="2dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="228dp"
        android:layout_marginEnd="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/login_line" />

    <ImageView
        android:id="@+id/signin_button2"
        android:layout_width="330dp"
        android:layout_height="46dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView23"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/signin_button" />

    <TextView
        android:id="@+id/textView96"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:fontFamily="@font/pretendard_regular"
        android:text="회원가입"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/signin_button2"
        app:layout_constraintEnd_toEndOf="@+id/signin_button2"
        app:layout_constraintStart_toStartOf="@+id/signin_button2"
        app:layout_constraintTop_toTopOf="@+id/signin_button2" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="49dp"
        android:layout_height="49dp"
        android:backgroundTint="#FEFAFD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.06999999"
        app:srcCompat="@drawable/exit_button" />

    <TextView
        android:id="@+id/textView67"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="130dp"
        android:fontFamily="@font/pretendard_semibold"
        android:text="회원가입"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="330dp"
        android:layout_height="2dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView33"
        app:srcCompat="@drawable/login_line" />

    <EditText
        android:id="@+id/et_email"
        android:layout_width="298dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/pretendard_regular"
        android:hint="이메일을 입력해주세요"
        android:inputType="textEmailAddress"
        android:textColor="#C4C4C4"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView35"
        app:layout_constraintStart_toEndOf="@+id/imageView36"
        app:layout_constraintTop_toBottomOf="@+id/textView67"
        app:layout_constraintVertical_bias="0.81" />

    <ImageView
        android:id="@+id/imageView36"
        android:layout_width="20dp"
        android:layout_height="16dp"
        android:layout_marginTop="30dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView35"
        app:layout_constraintStart_toStartOf="@+id/imageView35"
        app:layout_constraintTop_toBottomOf="@+id/textView67"
        app:srcCompat="@drawable/email_icon" />

    <EditText
        android:id="@+id/et_pwd"
        android:layout_width="298dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/pretendard_regular"
        android:hint="비밀번호를 입력해주세요"
        android:inputType="textPassword"
        android:textColor="#C4C4C4"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView39"
        app:layout_constraintStart_toEndOf="@+id/imageView39"
        app:layout_constraintTop_toTopOf="@+id/imageView39"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView39"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="15dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView24"
        app:layout_constraintStart_toStartOf="@+id/imageView24"
        app:layout_constraintTop_toBottomOf="@+id/imageView35"
        app:layout_constraintVertical_bias="0.39"
        app:srcCompat="@drawable/lock_icon" />

    <EditText
        android:id="@+id/et_name"
        android:layout_width="298dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/pretendard_regular"
        android:hint="이름을 입력해주세요"
        android:textColor="#C4C4C4"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView40"
        app:layout_constraintStart_toEndOf="@+id/imageView40"
        app:layout_constraintTop_toTopOf="@+id/imageView40"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView40"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="18dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView33"
        app:layout_constraintStart_toStartOf="@+id/imageView33"
        app:layout_constraintTop_toBottomOf="@+id/imageView24"
        app:layout_constraintVertical_bias="0.23000002"
        app:srcCompat="@drawable/profile_icon" />

    <ImageView
        android:id="@+id/imageView41"
        android:layout_width="20dp"
        android:layout_height="22dp"
        android:layout_marginTop="14dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView23"
        app:layout_constraintStart_toStartOf="@+id/imageView23"
        app:layout_constraintTop_toBottomOf="@+id/imageView33"
        app:srcCompat="@drawable/phone_icon" />
</androidx.constraintlayout.widget.ConstraintLayout>